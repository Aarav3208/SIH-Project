{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src", // Ensure this is correct
    "paths": {
      "@mui/*": ["node_modules/@mui/*"],
      "@data/*": ["data/*"],
      "@pages/*": ["pages/*"],
      "@assets/*": ["assets/*"],
      "@styles/*": ["styles/*"],
      "@components/*": ["components/*"]
    }
  },
  "include": ["src"]
}
